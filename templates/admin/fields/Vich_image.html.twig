{% if field.value is iterable %}
  {% for image in field.value %}
    {% set html_id = 'ea-lightbox' ~ field.uniqueId %}
    <a href="#" class="ea-lightbox-thumbnail" data-featherlight="#{{ html_id }}"
       data-featherlight-close-on-click="anywhere">
      <img src="{{ vich_uploader_asset(image, 'imageFile') }}" class="img-fluid">
    </a>
    <div id="{{ html_id }}" class="ea-lightbox">
      <img src="{{ vich_uploader_asset(image, 'imageFile') }}">
    </div>
  {% endfor %}
{% else %}
  {% set html_id = 'ea-lightbox' ~ field.uniqueId %}
  <a href="#" class="ea-lightbox-thumbnail" data-featherlight="#{{ html_id }}"
     data-featherlight-close-on-click="anywhere">
    <img src="{{ vich_uploader_asset(field.value, 'imageFile') }}" class="img-fluid">
  </a>
  <div id="{{ html_id }}" class="ea-lightbox">
    <img src="{{ vich_uploader_asset(field.value, 'imageFile') }}">
  </div>
{% endif %}